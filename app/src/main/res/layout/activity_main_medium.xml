<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".MainActivityMedium">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="133dp"
        android:layout_height="49dp"
        android:text="Time: "
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.201"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="144dp"
        android:layout_height="55dp"
        android:text="Score: "
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <Button
        android:id="@+id/back3"
        android:layout_width="90dp"
        android:layout_height="39dp"
        android:text="BACK"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="120dp"
        android:layout_height="173dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="73dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toTopOf="@+id/imageView30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView32"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rick3" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="126dp"
        android:layout_height="131dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="162dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toTopOf="@+id/imageView31"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.024"
        app:layout_constraintStart_toEndOf="@+id/back3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rick3" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="160dp"
        android:layout_height="145dp"
        android:layout_marginBottom="7dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toTopOf="@+id/imageView31"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back3"
        app:layout_constraintVertical_bias="0.791"
        app:srcCompat="@drawable/rick3" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="112dp"
        android:layout_height="143dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="180dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView28"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rick3" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="135dp"
        android:layout_height="161dp"
        android:layout_marginBottom="166dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView26"
        app:srcCompat="@drawable/rick3" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="118dp"
        android:layout_height="152dp"
        android:layout_marginEnd="97dp"
        android:layout_marginBottom="66dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView32"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView27"
        app:srcCompat="@drawable/rick3" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="110dp"
        android:layout_height="132dp"
        android:onClick="increaseScore"
        app:layout_constraintBottom_toBottomOf="@+id/imageView30"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView31"
        app:layout_constraintTop_toBottomOf="@+id/imageView30"
        app:srcCompat="@drawable/rick3" />

</androidx.constraintlayout.widget.ConstraintLayout>